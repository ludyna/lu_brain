// Cell Alloc

	Lu_N_Cell_Base n_cell__alloc_0(Lu_N_Cell_Allocator self)
	{
		lu__assert(self);
		lu__assert(self->mt_cell_0);

		Lu_N_Cell_Base cell = (Lu_N_Cell_Base) lu_mem_record__alloc(self->mt_cell_0);
		lu__assert(cell);

		lu_n_cell_base__init(cell, N_CT__0);

		return cell;
	}

