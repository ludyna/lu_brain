// Wave Story

	void lu_wave_block_begin(Lu_Wave self)
	{
		lu_user_assert_void(self, "Lu_Wave is NULL");

		if (self->block_begin)
			self->block_begin(self);
	}

	void lu_wave_block_end(Lu_Wave self)
	{
		lu_user_assert_void(self, "Lu_Wave is NULL");

		if (self->block_end)
			self->block_end(self);
	}

