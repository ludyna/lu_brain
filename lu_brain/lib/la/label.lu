	{
		if(!self)
		{
		 	if(lu_g_user_assert) 
		 	{
		 	 	lu__user_debug("Lu_Label is NULL");
		 	}

		 	return LU__SIZE_MAX;
		}

		return lu_la_cell__get_ix(self->la_cell);
	}